<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:cardCornerRadius="5dp"
    app:cardElevation="5dp"
    android:layout_marginHorizontal="10dp"
    android:layout_marginVertical="5dp"
    android:background="@drawable/card_border"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@id/textRingkasan">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="20dp"
            android:layout_marginVertical="15dp"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/gambarAAktivitas"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_marginVertical="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_gravity="center_vertical"
                    app:srcCompat="@drawable/home_menu5"
                    android:scaleType="centerCrop"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tipeAAktivitas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:fontFamily="@font/amethysta"
                        android:gravity="center_vertical"
                        android:text="Daihatsu Sigra"
                        android:textColor="@color/colorPrimaryDark"
                        android:textStyle="bold"
                        tools:layout_editor_absoluteX="133dp"
                        tools:layout_editor_absoluteY="110dp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Tahun Kendaraan : "
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                        <TextView
                            android:id="@+id/tahunAAktivitas"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="2019"
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Tujuan : "
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                        <TextView
                            android:id="@+id/tujuanAktivitas"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Jakarta"
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Dari : "
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                        <TextView
                            android:id="@+id/berangkatAktivitas"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Senin"
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="5dp"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Sampai : "
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                        <TextView
                            android:id="@+id/pulangAktivitas"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/amethysta"
                            android:gravity="center_vertical"
                            android:text="Jumat"
                            android:textColor="#070707"
                            tools:layout_editor_absoluteX="133dp"
                            tools:layout_editor_absoluteY="136dp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginBottom="10dp"
                android:background="#8F8B8B"
                tools:layout_editor_absoluteX="35dp"
                tools:layout_editor_absoluteY="416dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/amethysta"
                android:text="Total Biaya"
                android:textColor="#0C0C0C"
                android:textSize="14dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="37dp"
                tools:layout_editor_absoluteY="179dp" />

            <TextView
                android:id="@+id/biayaAktivitas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="Rp. 30.000.000,00"
                android:fontFamily="@font/amethysta"
                android:textColor="#E8592D"
                android:textSize="15dp"
                tools:layout_editor_absoluteX="103dp"
                tools:layout_editor_absoluteY="212dp" />

        </LinearLayout>
    </ScrollView>
</androidx.cardview.widget.CardView>

